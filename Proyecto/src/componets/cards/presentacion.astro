<style>
  div {
    display: flex;
    width: 100%;
    height: 100%;
  }

  section {
    width: 60%;
    padding: 20px;
    color: white;
    font-size: 30px;
    display: flex;
    flex-direction: column;
    justify-content: space-evenly;
  }

  picture img {
    width: 100%;
    height: 100%;
    border-radius: 0 40px 40px 0;
  }

  .title {
    font-family: var(--title_prin);
    font-size: 1em;
  }

  .descripcion,
  .data_personal,
  .link_cv {
    font-size: 0.5em;
    font-family: var(--paragraph);
  }

  .link_cv {
    width: 40%;
    text-align: center;
    border: 2px solid white;
    color: white;
    border-radius: 30px;
    padding: 5px;
    transition: all 1s;
    text-decoration: none;
  }

  aside {
    display: flex;
    align-items: center;
  }

  aside,
  p {
    margin: 10px 0;
  }

  aside p {
    font-weight: 900;
  }

  aside span {
    font-weight: 100;
  }

  .link_cv:hover {
    background-color: var(--background_hv);
    cursor: pointer;
  }

  @media screen and (max-width: 850px) {
    div {
      flex-direction: column;
      width: 100%;
    }

    section {
      width: 100%;
    }

    picture{
      display: flex;
      border-radius: 0 0 40px 40px;
    }

    picture img {
      width: 100%;
      border-radius: 0 0 40px 40px;
      
    }

    .title {
      font-family: var(--title_prin);
      font-size: 0.7em;
    }

    .descripcion,
    .data_personal,
    .link_cv {
      font-size: 0.4em;
    }
  }
</style>
