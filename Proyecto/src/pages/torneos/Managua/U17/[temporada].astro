---
import temaJSON from "../../../../JSON/team.json";
import TorneosLy from "../../../../layout/torneosLy.astro";

export function getStaticPaths() {
  return [
    { params: { temporada: "2021" } },
    { params: { temporada: "2022" } },
    { params: { temporada: "2023" } },
  ];
}

const { temporada } = Astro.params;
// Consumir el json
const aux = temaJSON.map((info) => info.Managua.sub17[temporada]);

// Trabajar de una forma mÃ¡s limpia
const data = aux[0];
---

{/*  Se encuentra en la carpeta de src/Layout */}
<TorneosLy data={data}></TorneosLy>
